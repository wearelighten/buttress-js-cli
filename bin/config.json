{
  "environment": {
    "NODE_ENV": "production",
    "PROTOCOL": "https",
    "URL": "",
    "TOKEN": "",
    "API_PATH": "bjs",
    "API_VERSION": 1,
    "ALLOW_UNAUTHORIZED": "false"
  },
  "global": {
    "env": "%NODE_ENV%",
    "app": {
      "title": "ButtressJS CLI",
      "description": "Basic CLI app for interacting with Buttress.",
      "code": "bjs-cli",
      "version": "1.3.1"
    },
    "auth": {
      "buttress": {
        "url": "%PROTOCOL%://%URL%",
        "appToken": "%TOKEN%",
        "apiPath": "%API_PATH%",
        "apiVersion": "%API_VERSION%",
        "allowUnauthorized": "%ALLOW_UNAUTHORIZED%"
      }
    }
  }
}
